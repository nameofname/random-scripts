{
  "manifest_version": 3,
  "name": "DomDestroyer",
  "description": "Destroy parts of the dom with a click",
  "version": "1",
  "author": "nameofname",
  "permissions": ["activeTab"],
  "action": {
    "default_title": "Notifier",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["bundle.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
 }
